{
  "name": "moon-language",
  "displayName": "Moon Language Support",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/1vicxtr/ext-vsc-moon.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:mn"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mn",
        "aliases": [
          "Moon",
          "mn"
        ],
        "extensions": [
          ".mn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
     "iconThemes": [
      {
        "id": "moon-icons",
        "label": "Moon Icons",
        "path": "./file-icons.json"
      }
    ],
    "grammars": [
      {
        "language": "mn",
        "scopeName": "source.moon",
        "path": "./syntaxes/moon.tmLanguage.json"
      }
    ]
  }
}
