{
  "name": "moon-language",
  "displayName": "Moon Language Support",
  "version": "0.0.1",
  "repository": {
  "type": "git",
  "url": "https://github.com/1vicxtr/ext-vsc-moon.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:moon"
  ],
  "contributes": {
    "languages": [{
      "id": "moon",
      "aliases": ["Moon", "moon"],
      "extensions": [ ".moon" ],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "moon",
      "scopeName": "source.moon",
      "path": "./syntaxes/moon.tmLanguage.json"
    }]
  }
}